<library name="corevector" version="1.0.0" language="Java">
    <node name="rect" class="net.nodebox.corevector.Rect">
        <group label="Basic">
            <parameter name="x" type="float"/>
            <parameter name="y" type="float"/>
            <parameter name="width" type="float" default="100"/>
            <parameter name="height" type="float" default="100"/>
        </group>
        <group label="Color">
            <parameter name="filled" type="boolean"/>
            <parameter name="fillColor" type="color"/>
            <parameter name="stroked" type="boolean"/>
            <parameter name="strokeColor" type="color"/>
            <parameter name="strokeWidth" type="float"/>
        </group>
    </node>
</library>